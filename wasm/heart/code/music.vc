void FadeMusic(string to)
{
	int song;
	if (strcmp(musicfn,to)!=0)
	{
		song=LoadSong(to);
		timer=0;
		while(timer<=50) { Sleep(0); SetSongVolume(music, 50-timer); }
		StopSong(music);
		musicfn=to;
		music=song;
		PlaySong(music);
		timer=0;
		while(timer<=50) { Sleep(0); SetSongVolume(music, timer); }
	}
}

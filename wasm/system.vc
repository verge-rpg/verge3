void AutoExec()
{
    int img = LoadImage("test.png");

    Unpress(0);

    while (!b1)
    {    
        Blit(0, 0, img, screen);
        SetLucent(50);
        RectFill(0, ImageHeight(screen) / 2 - 8, ImageWidth(screen), ImageHeight(screen) / 2 + 8, RGB(timer % 256, 0, 255), screen);
        SetLucent(0);
        PrintString(8, ImageHeight(screen) / 2 - 8, screen, 0, "Hello world!");

        ShowPage();
    }

    Exit("Hello world!");
}